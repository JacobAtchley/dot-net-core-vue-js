<template id="people-list">
  <h2>People</h2>
    <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <table class="table responsive">
        <thead>
          <tr>
            <th>
              Id
            </th>
            <th>
              First Name
            </th>
            <th>
              Last Name
            </th>
            <th>
                Email Address
            </th>
        </thead>
        <tbody>
          <tr v-for="person in people ">
            <td>{{ person.id }}</td>
            <td>{{ person.firstName }}</td>
            <td>{{ person.lastName }}</td>
            <td>{{ person.email }}</td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</template>